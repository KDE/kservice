#: kbuildsycoca/kbuildsycoca.cpp:652
msgctxt "application name"
msgid "KBuildSycoca"
msgstr "KBuildSycoca"

#: kbuildsycoca/kbuildsycoca.cpp:654
msgctxt "application description"
msgid "Rebuilds the system configuration cache."
msgstr "Odśwież buforowaną konfigurację systemową."

#: kbuildsycoca/kbuildsycoca.cpp:656
msgctxt "@info:credit"
msgid "Copyright 1999-2014 KDE Developers"
msgstr "Copyright 2000 - 2014, Autorzy KDE"

#: kbuildsycoca/kbuildsycoca.cpp:657
msgctxt "@info:credit"
msgid "David Faure"
msgstr "David Faure"

#: kbuildsycoca/kbuildsycoca.cpp:658 kbuildsycoca/kbuildsycoca.cpp:661
msgctxt "@info:credit"
msgid "Author"
msgstr "Autor"

#: kbuildsycoca/kbuildsycoca.cpp:660
msgctxt "@info:credit"
msgid "Waldo Bastian"
msgstr "Waldo Bastian"

#: kbuildsycoca/kbuildsycoca.cpp:671
msgctxt "@info:shell command-line option"
msgid "Do not signal applications to update"
msgstr "Nie zgłaszaj uaktualnienia programom"

#: kbuildsycoca/kbuildsycoca.cpp:674
msgctxt "@info:shell command-line option"
msgid "Disable incremental update, re-read everything"
msgstr "Wyłącz przyrostowe uaktualnianie, przeczytaj wszystko"

#: kbuildsycoca/kbuildsycoca.cpp:677
msgctxt "@info:shell command-line option"
msgid "Check file timestamps"
msgstr "Sprawdź datę pliku"

#: kbuildsycoca/kbuildsycoca.cpp:680
msgctxt "@info:shell command-line option"
msgid "Disable checking files (dangerous)"
msgstr "Wyłącz sprawdzanie plików (niebezpieczne)"

#: kbuildsycoca/kbuildsycoca.cpp:683
msgctxt "@info:shell command-line option"
msgid "Create global database"
msgstr "Utwórz bazę globalną"

#: kbuildsycoca/kbuildsycoca.cpp:686
msgctxt "@info:shell command-line option"
msgid "Perform menu generation test run only"
msgstr "Wykonaj jedynie próbę utworzenia menu"

#: kbuildsycoca/kbuildsycoca.cpp:689
msgctxt "@info:shell command-line option"
msgid "Track menu id for debug purposes"
msgstr "Śledź identyfikator menu na potrzeby debugowania"

#: kbuildsycoca/kbuildsycoca.cpp:693
msgctxt "@info:shell command-line option"
msgid "Switch QStandardPaths to test mode, for unit tests only"
msgstr ""
"Przełącz QStandardPaths w tryb wypróbowywania, tylko dla prób jednostkowych"

#: kdeinit/ktoolinvocation.cpp:66
msgid "Function must be called from the main thread."
msgstr "Funkcja musi być wywoływana z głównego wątku."

#: kdeinit/ktoolinvocation.cpp:103
#, kde-format
msgid ""
"Error launching %1. Either KLauncher is not running anymore, or it failed to "
"start the application."
msgstr ""
"Błąd podczas uruchamiania %1. Albo program KLauncher już nie działa, albo "
"nie mógł uruchomić aplikacji."

#: kdeinit/ktoolinvocation.cpp:106
#, kde-format
msgid ""
"KLauncher could not be reached via D-Bus. Error when calling %1:\n"
"%2\n"
msgstr ""
"KLauncher był niedostępny dla D-Bus, błąd przy wywołaniu %1:\n"
"%2\n"

